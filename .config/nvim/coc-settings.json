{
  "coc.source.emoji.filetypes": ["markdown"],
  "diagnostic": {
    "enable": true,
    "errorSign": "✘",
    "warningSign": "⚠",
    "infoSign": "ⓘ",
    "enableMessage": "always",
    "messageTarget": "float",
    "virtualText": false,
    "checkCurrentLine": true
  },
  "suggest": {
    "autoTrigger": "trigger",
    "minTriggerInputLength": 2,
    "snippetIndicator": "►"
  },
  "javascript.suggestionActions.enabled": false,
  "typescript.suggestionActions.enabled": false
}
